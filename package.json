{
  "name": "behringer-remote",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "lodash": "^4.17.21",
    "osc": "^2.4.4",
    "prompt-sync": "^4.2.0",
    "qrcode-terminal": "^0.12.0",
    "socket.io": "^4.6.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.3"
  },
  "scripts": {
    "start": "ts-node src/index.ts",
    "dev": "nodemon --esm src/index.ts",
    "build": "bash ./scripts/build.sh"
  },
  "devDependencies": {
    "@types/node": "^18.15.11",
    "nodemon": "^2.0.22",
    "pkg": "^5.8.1"
  },
  "pkg": {
    "scripts": "build/**/*.js",
    "assets": "./public/**/*",
    "targets": [
      "node18-linux-x64",
      "node18-win",
      "node18-macos-x64"
    ],
    "outputPath": "build"
  },
  "bin": "build/src/index.js"
}
